<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:ServiceApplication.MVVM.ViewModels"
                    xmlns:views="clr-namespace:ServiceApplication.MVVM.Views"
                    xmlns:viewModels1="clr-namespace:SharedLibrary.MVVM.ViewModels;assembly=SharedLibrary"
                    xmlns:views1="clr-namespace:ServiceApplication.MVVM.Views;assembly=SharedLibrary"
                    xmlns:viewModels2="clr-namespace:ServiceApplication.MVVM.ViewModels;assembly=SharedLibrary">

    <DataTemplate DataType="{x:Type viewModels1:HomeViewModel}">
        <views1:HomeView/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type viewModels2:SettingsViewModel}">
        <views1:SettingsView/>
    </DataTemplate>



</ResourceDictionary>